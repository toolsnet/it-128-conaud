<p id="notice"><%= notice %></p>

<h1>Tipos de Documentos</h1>

<table>
  <thead>
    <tr>
      <th>Id tip doc</th>
      <th>Nom tip doc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tip_docs.each do |tip_doc| %>
      <tr>
        <td><%= tip_doc.id_tip_doc %></td>
        <td><%= tip_doc.nom_tip_doc %></td>
        <td><%= link_to 'Show', tip_doc %></td>
        <td><%= link_to 'Edit', edit_tip_doc_path(tip_doc) %></td>
        <td><%= link_to 'Destroy', tip_doc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tip Doc', new_tip_doc_path %>
