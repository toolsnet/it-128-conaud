<p id="notice"><%= notice %></p>

<h1>Denuncia</h1>

<table>
  <thead>
    <tr>
      <th>Id den</th>
      <th>Id usu den</th>
      <th>Lat den</th>
      <th>Lon den</th>
      <th>Ima fue</th>
      <th>Aud rep</th>
      <th>Aud niv</th>
      <th>Fec den</th>
      <th>Hor den</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @denuncia.each do |denuncium| %>
      <tr>
        <td><%= denuncium.id_den %></td>
        <td><%= denuncium.id_usu_den %></td>
        <td><%= denuncium.lat_den %></td>
        <td><%= denuncium.lon_den %></td>
        <td><%= denuncium.ima_fue %></td>
        <td><%= denuncium.aud_rep %></td>
        <td><%= denuncium.aud_niv %></td>
        <td><%= denuncium.fec_den %></td>
        <td><%= denuncium.hor_den %></td>
        <td><%= link_to 'Show', denuncium %></td>
        <td><%= link_to 'Edit', edit_denuncium_path(denuncium) %></td>
        <td><%= link_to 'Destroy', denuncium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Denuncium', new_denuncium_path %>
